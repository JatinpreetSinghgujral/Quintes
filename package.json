{
  "name": "QuantisTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "x": "react-native run-ios --simulator='iPhone X'",
    "i": "react-native run-ios",
    "a": "react-native run-android",
    "reset": "react-native start --reset-cache",
    "rename": "npx react-codemod rename-unsafe-lifecycles",
    "ggc": "git config --global  --get core.ignorecase",
    "glc": "git config --local  --get core.ignorecase",
    "sgc": "git config --global core.ignorecase false",
    "slc": "git config --local core.ignorecase false",
    "fix": "cp fixes/index.js node_modules/react-native-swiper/src/",
    "pi": "pod install --project-directory=$( dirname '${BASH_SOURCE[0]}' )/ios;npm run i;",
    "pa": "pod install --project-directory=$( dirname '${BASH_SOURCE[0]}' )/ios;npm run a;",
    "backup": "git archive --format zip --output ./backup.zip master"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.7.1",
    "@react-native-community/netinfo": "3.1.0",
    "@react-native-community/viewpager": "^3.3.0",
    "@valdio/react-native-scrollable-tabview": "^0.8.12",
    "aws-sdk": "^2.675.0",
    "axios": "^0.19.2",
    "moment": "2.24.0",
    "moment-precise-range-plugin": "^1.3.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-calendars": "1.22.0",
    "react-native-cardview": "^2.0.5",
    "react-native-collapsible": "^1.5.3",
    "react-native-dash": "0.0.9",
    "react-native-device-info": "^5.5.7",
    "react-native-document-picker": "^3.3.3",
    "react-native-firebase": "^5.5.6",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-image-crop-picker": "^0.28.0",
    "react-native-keyboard-aware-scroll-view": "0.8.0",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-localization": "^2.1.6",
    "react-native-modal-datetime-picker": "6.1.0",
    "react-native-reanimated": "1.4.0",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-simple-toast": "^1.1.2",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swiper": "1.5.14",
    "react-native-webview": "^8.0.2",
    "react-navigation": "4.0.10",
    "react-navigation-drawer": "^2.4.10",
    "react-navigation-stack": "1.10.3",
    "react-navigation-tabs": "2.6.2",
    "react-redux": "7.0.3",
    "redux": "4.0.1",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/runtime": "7.11.2",
    "@react-native-community/eslint-config": "1.1.0",
    "babel-jest": "25.5.1",
    "eslint": "6.8.0",
    "jest": "25.5.4",
    "metro-react-native-babel-preset": "0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
